{
  "version": 2,
  "name": "home-cljs",
  "alias": ["ackl.in", "jamesacklin.com"],
  "builds": [
    {
      "src": "shadow-cljs.edn",
      "use": "@jntn/now-shadow-cljs"
    }
  ],
  "routes": [
    {
      "src": "/about",
      "status": 301,
      "headers": { "Location": "/#/about" }
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}